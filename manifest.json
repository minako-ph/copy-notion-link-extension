{
  "name": "Copy Notion Item Link",
  "author": "minako",
  "description": "Notion内にあるアイテムのリンクをタイトルとセットにしてクリッボードにコピーします",
  "version": "1.0.1",
  "manifest_version": 2,
  "web_accessible_resources": ["*"],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.notion.so/*"],
      "css": ["css/style.css"],
      "js": ["js/script.js"]
    }
  ]
}
